  {
    "manifest_version": 3,
    "name": "AI Auto-Fill",
    "version": "1.0",
    "description": "Automatically fills web forms using AI.",
    "action": {
      "default_popup": "popup.html"      
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  
  