  {
    "manifest_version": 3,
    "name": "AI Auto-Fill",
    "version": "1.0",
    "description": "Automatically fills web forms using AI.",
    "action": {
      "default_popup": "popup.html"      
    },
    "icons": {
      "16": "icons/logo.png",
      "48": "icons/logo.png",
      "128": "icons/logo.png"
    },
    "options_ui": {
      "page": "options.html",
      "open_in_tab": true
    },
    "permissions": [
      "activeTab",
      "scripting",
      "storage",
      "contextMenus"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  
  